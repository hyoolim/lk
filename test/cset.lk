/test['cs - E-S'; cs - 'E-S' ->to string; 'A-DV-Z']
/test['cs - E-T'; cs - 'E-T' ->to string; 'A-DV-Z']
/test['cs - E-U'; cs - 'E-U' ->to string; 'A-DV-Z']
/test['cs - F'  ; cs - 'F'   ->to string; 'A-DH-KO-RV-Z']
