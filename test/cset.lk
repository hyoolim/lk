/test['cs - 0-9'; cs - '0-9' ->to string; 'A-DH-KO-RV-Z']
/test['cs - 0-C'; cs - '0-C' ->to string; 'DH-KO-RV-Z']
/test['cs - 0-E'; cs - '0-E' ->to string; 'H-KO-RV-Z']
/test['cs - 0-F'; cs - '0-F' ->to string; 'H-KO-RV-Z']
/test['cs - 0-G'; cs - '0-G' ->to string; 'H-KO-RV-Z']
/test['cs - a-z'; cs - 'a-z' ->to string; 'A-DH-KO-RV-Z']
/test['cs - P-z'; cs - 'P-z' ->to string; 'A-DH-KO']
/test['cs - S-z'; cs - 'S-z' ->to string; 'A-DH-KO-R']
/test['cs - T-z'; cs - 'T-z' ->to string; 'A-DH-KO-R']
