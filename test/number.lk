include('tester.lk')
Tester/new('Number')/do {

/test('1 + 2', 1 + 2, 3)
/test('1 + 2 * 3', 1 + 2 * 3, 7)
/test('(1 + 2) * 3', (1 + 2) * 3, 9)
/test('1 + 2.1', 1 + 2.1, 3.1)
/test('1.1 + 2', 1.1 + 2, 3.1)
/test('1.1 + 2.1', 1.1 + 2.1, 3.2)
/test('- - 5', - - 5, 5)
/test('123.456 * 123.456', 123.456 * 123.456, 15241.383936)
/test('1 < 1', 1 < 1, False)
/test('1 == 1', 1 == 1, True)
/test('1 > 1', 1 > 1, False)
/test('1 < 1.0', 1 < 1.0, False)
/test('1 == 1.0', 1 == 1.0, True)
/test('1 > 1.0', 1 > 1.0, False)
/test('floor #1', 1.1/floor, 1)
/test('floor #2', -1.1/floor, -2)
/test('floor #3', -1.0/floor, -1)
/test('ceil #1', 1.1/ceil, 2)
/test('ceil #2', -1.1/ceil, -1)
/test('ceil #3', -1.0/ceil, -1)
/test('round #1', 1.1/round, 1)
/test('round #2', -1.1/round, -1)
/test('round #3', -1.6/round, -2)
/finish
}
